{
  "extends": "tslint:latest",
  "linterOptions": {
    "exclude": [
      "src/demo.ts"
    ]
  },
  "rules": {
    "trailing-comma": [true, {"multiline": "always", "singleline": "never"}],
    "no-unused-expression": false,
    "max-line-length": false,
    "ordered-imports": true,
    "no-console": false,
    "no-var-requires": false,
    "quotemark": [true, "single"]
  }
}